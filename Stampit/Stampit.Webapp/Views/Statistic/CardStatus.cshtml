@{
    var myChart = new Chart(width: 600, height: 400)
       .AddTitle("Cardstatus per product")
       .SetXAxis(title: "Amount of stamps", min: 0)
       .SetYAxis(title: "Amount of cards", min: 0, max: 10)
       .AddSeries(chartType: "bar", axisLabel: "hallo",
          xValue: new[] { "< 6", "6", "7", "8", "9", "10" },
